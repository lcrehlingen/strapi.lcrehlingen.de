{
  "kind": "collectionType",
  "collectionName": "sponsors",
  "info": {
    "singularName": "sponsor",
    "pluralName": "sponsors",
    "displayName": "Sponsor",
    "name": "sponsor"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
