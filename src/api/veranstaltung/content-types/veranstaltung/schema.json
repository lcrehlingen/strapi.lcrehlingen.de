{
  "kind": "collectionType",
  "collectionName": "veranstaltungs",
  "info": {
    "singularName": "veranstaltung",
    "pluralName": "veranstaltungs",
    "displayName": "Veranstaltung",
    "name": "veranstaltung"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "image": {
      "type": "media",
      "allowedTypes": [
        "images"
      ],
      "multiple": false,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "results": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "content": {
      "type": "richtext"
    }
  }
}
